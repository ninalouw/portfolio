
{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "ecmaFeatures": {
        "classes": true,
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": [2, { skipUndeclared: true }],
    "arrow-body-style": ["error", "always"],
    "no-underscore-dangle": ["error", { "allow": ["_render"] }]
  },
  "env": {
        "browser": true
  }
}
